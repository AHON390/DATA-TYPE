'''
game_info={
    'player':{
    'name':"dark",
    'level':'590',
    'health':113,
    'inventory':['sword','sheild','potion']
},
    'player2':{
    'name':"hyper_dip",
    'level':'59',
    'health':80,
    'inventory':['sword','potion']
}

}
for player_id,info in game_info.items():
    for key,value in game_info.items():
        print(f"{key}:{value}")
