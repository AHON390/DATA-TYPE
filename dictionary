'''
marks_of_number={'ahon':97,'miftahul':23,'rajveer':79}
print("the original dictionary: "+ str(marks_of_number))
k=2
res=1
for key in marks_of_number:
    if marks_of_number[key]==k:
        res=res+1
print("frequently of k is:"+ str(res))
